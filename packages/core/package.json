{
  "name": "@bridge/core",
  "version": "0.2.2",
  "packageManager": "pnpm@8.15.3",
  "type": "module",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "test": "vitest --",
    "build": "pnpm build-vue && pnpm dlx tsc-alias -p vue/tsconfig.json",
    "build-vue": "rimraf vue/temp && cd vue && pnpm tsc --noCheck || exit 0;",
    "release": "release-it"
  },
  "exports": {
    ".": {
      "default": "./src/index.js",
      "types": "./src/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "imports": {
    "#vue-internals/reactivity/*": "./vue/temp/packages/reactivity/src/*.js",
    "#vue-internals/runtime-core/*": "./vue/temp/packages/runtime-core/src/*.js",
    "#vue-internals/shared/*": "./vue/temp/packages/shared/src/*.js",
    "#vue-internals": "./vue/temp/packages/vue/src/*.js",
    "#vue/compat": "./vue/temp/packages/vue-compat/src/index.js",
    "#vue/reactivity": "./vue/temp/packages/reactivity/src/index.js",
    "#vue/runtime-core": "./vue/temp/packages/runtime-core/src/index.js",
    "#vue/runtime-dom": "./vue/temp/packages/runtime-dom/src/index.js",
    "#vue/runtime-test": "./vue/temp/packages/runtime-test/src/index.js",
    "#vue/server-renderer": "./vue/temp/packages/server-renderer/src/index.js",
    "#vue/shared": "./vue/temp/packages/shared/src/index.js",
    "#vue/vue": "./vue/temp/packages/vue/src/index.js",
    "#vue/vue-compat": "./vue/temp/packages/vue-compat/src/index.js",
    "#vue/*": "./vue/temp/packages/*/src/index.js"
  },
  "sideEffects": false,
  "files": [
    "src",
    "vue",
    "../../vue/temp",
    "README.md"
  ],
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.26.2",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@types/react": "^18.3.12",
    "copyfiles": "^2.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.17.2",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2",
    "unplugin-inline-enum": "^0.2.0",
    "vite": "^6.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "release-it": {
    "git": false,
    "plugins": {
      "release-it-pnpm": {}
    }
  }
}
