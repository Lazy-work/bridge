{
  "name": "@briddge/core",
  "version": "0.3.13",
  "packageManager": "pnpm@8.15.3",
  "type": "module",
  "main": "./dist/briddge-core.cjs.js",
  "module": "./dist/briddge-core.esm-bundler.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "test": "vitest --",
    "build": "pnpm build-rm-alias && pnpm build-js",
    "build-js": "rimraf dist && pnpm rollup -c rollup.config.js",
    "build-vue": "(rimraf vue/temp && cd vue && pnpm tsc --noCheck) || exit 0;",
    "build-rm-alias": "(pnpm build-vue && pnpm dlx tsc-alias -p vue/tsconfig.json) || exit 0;",
    "release": "release-it"
  },
  "exports": {
    ".": {
      "import": {
        "default": "./dist/briddge-core.esm-bundler.js",
        "types": "./src/index.d.ts"
      },
      "require": {
        "default": "./dist/briddge-core.cjs.js",
        "types": "./src/index.d.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "imports": {
    "#vue-internals/reactivity/*.js": "./vue/temp/packages/reactivity/src/*.js",
    "#vue-internals/runtime-core/*.js": "./vue/temp/packages/runtime-core/src/*.js",
    "#vue-internals/shared/*.js": "./vue/temp/packages/shared/src/*.js",
    "#vue/reactivity.js": "./vue/temp/packages/reactivity/src/index.js",
    "#vue/runtime-core.js": "./vue/temp/packages/runtime-core/src/index.js",
    "#vue/shared.js": "./vue/temp/packages/shared/src/index.js"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "vue/temp/packages/runtime-core/src",
    "vue/temp/packages/reactivity/src",
    "vue/temp/packages/shared/src",
    "global.d.ts"
  ],
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.26.2",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@types/react": "^18.3.12",
    "copyfiles": "^2.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.17.2",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2",
    "unplugin-inline-enum": "^0.2.0",
    "vite": "^6.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "release-it": {
    "git": false,
    "plugins": {
      "release-it-pnpm": {}
    }
  }
}
