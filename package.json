{
  "name": "@lazywork/reactive-vue",
  "version": "0.0.0",
  "type": "module",
  "main": "index.js",
  "module": "./dist/reactive-vue.prod.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "test": "vitest --",
    "build": "run-s build:clean build:dev build:prod",
    "build:clean": "rimraf dist",
    "build:dev": "rollup -c ./rollup.config.dev.js",
    "build:prod": "rollup -c ./rollup.config.prod.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/reactive-vue.prod.js"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "types",
    "README.md"
  ],
  "dependencies": {
    "@vue/shared": "^3.4.25",
    "nanoid": "^5.0.6"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/react": "^18.2.56",
    "esbuild": "^0.20.2",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "react": "^18.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.17.2",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.2.2",
    "vitest": "^1.5.2"
  }
}
